{
    "name": "glcan/integration_continue",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Odin-srvn",
            "email": "odin.douteaux@hotmail.fr"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "composer-phar/php-cs-fixer": "^3.88",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.89"
    },
    "scripts": {
        "stan-check": "phpstan analyse src tests --level=5",
        "cs-check": "php-cs-fixer fix --dry-run --diff --using-cache=no src tests"
    }
}
